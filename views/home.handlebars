<table class="table table-bordered table-hover">

    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
        </tr>
    </thead>

    <tbody>
        {{#if students}}
        {{#each students}}
        <tr>
            <td>{{name}}</td>
            <td>{{age}}</td>
        </tr>
        {{/each}}
        {{/if}}
    </tbody>

</table>